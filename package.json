{
	"name": "svelte-kbar",
	"version": "0.0.10",
	"description": "A reusable k-bar component for your Svelte application.",
	"scripts": {
		"dev": "svelte-kit dev",
		"build": "svelte-kit build",
		"preview": "svelte-kit preview",
		"test": "jest src",
		"test:watch": "npm run test -- --watch",
		"package": "yarn run format && yarn run lint && svelte-kit package",
		"release": "cd package && npm publish",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .prettierignore .",
		"format": "prettier --write --plugin-search-dir=. ."
	},
	"homepage": "https://github.com/dwagio/svelte-kbar",
	"bugs": {
		"url": "https://github.com/dwagio/svelte-kbar/issues"
	},
	"license": "MIT",
	"author": {
		"name": "Tas Hasting",
		"email": "tas.hasting@gmail.com",
		"url": "https://github.com/dwagio"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/dwagio/svelte-kbar.git"
	},
	"keywords": [
		"svelte",
		"search",
		"navigation",
		"navbar",
		"component"
	],
	"jest": {
		"transform": {
			"^.+\\.js$": "babel-jest",
			"^.+\\.svelte$": [
				"svelte-jester",
				{
					"preprocess": true
				}
			],
			"^.+\\.ts$": "ts-jest"
		},
		"moduleFileExtensions": [
			"js",
			"ts",
			"svelte"
		],
		"setupFilesAfterEnv": [
			"@testing-library/jest-dom/extend-expect"
		],
		"testEnvironment": "jsdom",
		"transformIgnorePatterns": [
			"/node_modules/(?!(.+\\.svelte$))",
			"\\.pnp\\.[^\\/]+$"
		]
	},
	"devDependencies": {
		"@sveltejs/kit": "next",
		"@testing-library/jest-dom": "^5.14.1",
		"@testing-library/svelte": "^3.0.3",
		"@types/jest": "^27.0.2",
		"@typescript-eslint/eslint-plugin": "^4.31.1",
		"@typescript-eslint/parser": "^4.31.1",
		"babel-jest": "^27.2.5",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^3.2.1",
		"jest": "^27.2.5",
		"prettier": "^2.4.1",
		"prettier-plugin-svelte": "^2.4.0",
		"svelte": "^3.42.6",
		"svelte-check": "^2.2.6",
		"svelte-jester": "^2.1.5",
		"svelte-preprocess": "^4.9.8",
		"svelte2tsx": "^0.4.7",
		"ts-jest": "^27.0.5",
		"tslib": "^2.3.1",
		"typescript": "^4.4.3"
	},
	"type": "module",
	"dependencies": {
		"match-sorter": "^6.3.1",
		"svelte-portal": "^2.1.2"
	}
}
